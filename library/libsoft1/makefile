	clang -c -O2 calc/scientificnotation.c
	clang -c -O2 compress/7z.c
	clang -c -O2 compress/cpio.c
	clang -c -O2 compress/gz.c
	clang -c -O2 compress/tar.c
	clang -c -O2 compress/zip.c
	clang -c -O2 executable/elf.c
	clang -c -O2 executable/macho.c
	clang -c -O2 executable/pe.c
	clang -c -O2 filesystem/fs.ext.c
	clang -c -O2 filesystem/fs.fat.c
	clang -c -O2 filesystem/fs.hfs.c
	clang -c -O2 filesystem/fs.ntfs.c
