	rm -f *.o
	gcc -c -O2 -Wno-pointer-to-int-cast -Wno-int-to-pointer-cast gui.c
voice:
	rm -f *.o
	gcc -c -O2 -Wno-pointer-to-int-cast -Wno-int-to-pointer-cast web.c
web:
	rm -f *.o
	gcc -c -O2 -Wno-pointer-to-int-cast -Wno-int-to-pointer-cast web.c
clean:
	rm -f *.o
