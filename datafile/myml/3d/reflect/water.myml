<entity>
texball{
fmt:texball
arg:datafile/jpg/texball-skysphere.jpg
}
tree{
fmt:stl3d
arg:datafile/stl/spider-lowpoly.stl
}
water{
fmt:water
}
freecam{
fmt:freecam
}
virtual{
fmt:virtual
}
</entity>


<supply>
opengl{
fmt:wnd
title:"42"
width:1024
height:1024
depth:1024
}
</supply>


<style>
skyhaha{
}
skygeom{
