#glwnd depends on
(opengl, fullrect, 0) -> (freecam, 0, 0)
(opengl, uiuxrect, 0) -> (uiux, 0, 0)

#event
(camrts, 0, evto) -> (freecam, 0, evby)
(camrts, 0, evby) -> (uiux, 0, evto)
