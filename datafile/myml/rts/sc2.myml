<relation>
#virt depends on
(virt, gndgeom, 0) -> (ground, gndslot, 0)
(virt, treegeom, 0) -> (tree, treeslot, 0)
(virt, rabgeom, 0) -> (rabbit, rabslot, 0)
(virt, freegeom, 0) -> (freecam, freeslot, in)

#uiux depends on
(uiux, minirect, 0) -> (minimap, 0, 0)
