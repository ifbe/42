<relation>
(std, 0, dst) -> (xor, 0, src)
(std, 0, dst) -> (xnor, 0, src)

(pcb, notgeom, 0) -> (not, notslot, 0)
(pcb, memgeom, 0) -> (mem, memslot, 0)
(pcb, dffgeom, 0) -> (dff, dffslot, 0)

(pcb, norgeom, 0) -> (nor, norslot, 0)
(pcb, nandgeom, 0) -> (nand, nandslot, 0)
(pcb, xorgeom, 0) -> (xor, xorslot, 0)
(pcb, xnorgeom, 0) -> (xnor, xnorslot, 0)

(pcb, camgeom, 0) -> (cam, camslot, 0)
(opengl, rect, 0) -> (cam, 0, 0)
</relation>
