<entity>
nmos{
fmt:nmos
}
pmos{
fmt:pmos
}
sch{
fmt:digital
url:datafile/wiring/mos-not.wire
}

not{
fmt:not
}
cam{
fmt:freecam
}
world{
fmt:virtual
}
</entity>


<supply>
opengl{
fmt:wnd
}
std{
fmt:std
}
</supply>


<worker>
poller{
fmt:poller
}
</worker>


