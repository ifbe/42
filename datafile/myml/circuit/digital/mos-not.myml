}
</style>


<relation>
(std, 0, dst) -> (not, 0, ev)
(std, 0, dst) -> (sch, 0, ev)

(sch, 0, a) -> (pmos, 0, G)
(sch, 0, a) -> (nmos, 0, G)
(sch, 0, b) -> (pmos, 0, D)
(sch, 0, b) -> (nmos, 0, D)

(world, p0geom, 0) -> (pmos, p0slot, 0)
(world, n0geom, 0) -> (nmos, n0slot, 0)
(world, schgeom, 0) -> (sch, schslot, 0)
(world, notgeom, 0) -> (not, notslot, 0)

(world, camgeom, 0) -> (cam, camslot, 0)
(opengl, rect, 0) -> (cam, 0, 0)
</relation>
