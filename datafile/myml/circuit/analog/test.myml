<relation>
(V0, 0, p) -> (R0, 0, p)
(V0, 0, n) -> (R0, 0, n)

(pcb, v0geom, 0) -> (V0, v0slot, 0)
(pcb, r0geom, 0) -> (R0, r0slot, 0)
(pcb, skygeom, 0) -> (texball, skyslot, 0)
(pcb, camgeom, 0) -> (cam, camslot, 0)

(opengl, rect, 0) -> (cam, 0, 0)
</relation>
