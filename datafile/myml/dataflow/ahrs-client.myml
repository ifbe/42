<system>
udphost{
fmt:UDP
url:0.0.0.0:9999
}
tcpcli{
fmt:tcp
url:127.0.0.1:8888
}
</system>


<artery>
reline{
fmt:reline
}
str2fv{
fmt:str2fv
}
easyag{
fmt:easyag
}
fv2str{
fmt:fv2str
}
</artery>


<relation>
(udphost, 0, fd, dst) -> (reline, 0, art, src)
(reline, 0, art, dst) -> (str2fv, 0, art, src)
(str2fv, 0, art, dst) -> (easyag, 0, art, src)
(easyag, 0, art, dst) -> (fv2str, 0, art, src)
(fv2str, 0, art, dst) -> (tcpcli, 0, fd, src)
</relation>
