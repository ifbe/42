<device>
i2chost{
fmt:i2c
url:/dev/i2c-1
}
</device>


<system>
udphost{
fmt:UDP
url:0.0.0.0:9999
}
</system>


<artery>
lsm9ds1{
fmt:lsm9ds1
}
dbgf32{
fmt:dbgf32
}
</artery>


<relation>
(lsm9ds1, 0, art, clk) -> (udphost, 0, fd, dst)
(lsm9ds1, 0, art, i2c) -> (i2chost, 0, dev, dst)
(lsm9ds1, 0, art, dst) -> (dbgf32, 0, art, src)
</relation>
