<device>
i2chost{
fmt:i2c
url:/dev/i2c-1
}
</device>


<driver>
lsm9ds1{
fmt:lsm9ds1
}
</driver>


<system>
udphost{
fmt:UDP
url:0.0.0.0:9999
}
</system>


<artery>
dbgf32{
fmt:dbgf32
}
</artery>


<relation>
(lsm9ds1, 0, dri, clk) -> (udphost, 0, fd, dst)
(lsm9ds1, 0, dri, i2c) -> (i2chost, 0, dev, dst)
(lsm9ds1, 0, dri, dst) -> (dbgf32, 0, art, src)
</relation>
