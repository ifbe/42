<device>
i2chost{
fmt:i2c
url:/dev/i2c-1
}
</device>


<driver>
mpu9250{
fmt:mpu9250
url:0x68,0xc
}
</driver>


<artery>
mahony{
fmt:mahony
}
</artery>


<arena>
ahrs{
fmt:ahrs
}
</arena>


<relation>
(i2chost, 0, dev, 0) -> (mpu9250, 0, dri, i2c)
(mpu9250, 0, dri, dst) -> (mahony, 0, art, src)
(mahony, 0, art, dst) -> (ahrs, 0, win, src)
</relation>
