<device>
i2chost{
fmt:i2c
url:/dev/i2c-1
}
</device>


<driver>
mpu9250{
fmt:mpu9250
url:0x68,0xc
}
</driver>


<artery>
mahony{
fmt:mahony
}
</artery>


<supply>
ahrs{
fmt:ahrs
}
</supply>


<relation>
(i2chost, 0, 0) -> (mpu9250, 0, i2c)
(mpu9250, 0, dst) -> (mahony, 0, src)
(mahony, 0, dst) -> (ahrs, 0, src)
</relation>
