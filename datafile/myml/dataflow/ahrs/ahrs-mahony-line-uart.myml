<system>
uart{
fmt:uart
url:/dev/ttyUSB0
}
</system>


<artery>
reline{
fmt:reline
}
str2fv{
fmt:str2fv
}
mahony{
fmt:mahony
}
</artery>


<supply>
ahrs{
fmt:ahrs
}
</supply>


<relation>
(uart, 0, dst) -> (reline, 0, src)
(reline, 0, dst) -> (str2fv, 0, src)
(str2fv, 0, dst) -> (mahony, 0, src)
(mahony, 0, dst) -> (ahrs, 0, src)
</relation>
