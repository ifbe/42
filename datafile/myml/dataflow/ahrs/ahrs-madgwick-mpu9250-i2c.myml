<device>
i2chost{
fmt:i2c
url:/dev/i2c-1
}
</device>


<driver>
mpu9250{
fmt:mpu9250
url:0x68,0xc
}
</driver>


<artery>
madgwick{
fmt:madgwick
}
</artery>


<arena>
ahrs{
fmt:ahrs
}
</arena>


<relation>
(i2chost, 0, 0) -> (mpu9250, 0, i2c)
(mpu9250, 0, dst) -> (madgwick, 0, src)
(madgwick, 0, dst) -> (ahrs, 0, src)
</relation>
