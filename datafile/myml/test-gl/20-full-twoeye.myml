<entity>
sky{
fmt:texball
url:datafile/jpg/skysphere.jpg
}
gnd{
fmt:ground
}
dog{
fmt:stl3d
url:datafile/stl/dog-lowpoly.stl
}
left{
fmt:freecam
}
right{
fmt:freecam
}
world{
fmt:3dworld
}
</entity>


<supply>
glwnd{
fmt:gl41wnd0
}
opengl{
fmt:wnd
title:"42"
width:1024
height:1024
depth:1024
}
</supply>


<worker>
poller{
fmt:poller
}
</worker>


