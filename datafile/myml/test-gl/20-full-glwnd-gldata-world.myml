<relation>
#world depends on tree
(world, aaaageom, act, cam0) -> (cam0, aaaapart, act, 0)
(world, bbbbgeom, act, cam1) -> (cam1, bbbbpart, act, 0)
(world, treegeom, act, obj0) -> (tree, treepart, act, 0)
(world, skygeom, act, obj0) -> (sky, skypart, act, 0)

#gldata depends on world
(gldata, 0, act, all) -> (world, 0, act, all)

#glwnd depends on gldata
(glwnd, aaaarect, act, 0) -> (gldata, aaaafrus, act, cam0)
(glwnd, bbbbrect, act, 0) -> (gldata, bbbbfrus, act, cam1)

#opengl depends on glwnd
(opengl, 0, win, wnd) -> (glwnd, 0, act, togl)
</relation>
