<system>
uart{
fmt:uart
url:/dev/ttyUSB0
}
</system>


<artery>
reline{
fmt:reline
}
conv{
fmt:str2vec
}
mahony{
fmt:mahony
}
</artery>


<supply>
ahrs{
fmt:ahrs
}
</supply>


<relation>
(uart, 0, dst) -> (reline, 0, src)
(reline, 0, dst) -> (conv, 0, str)
(conv, 0, vec) -> (mahony, 0, src)
(mahony, 0, dst) -> (ahrs, 0, src)
</relation>
