<system>
client{
fmt:tcp
url:192.168.2.112:9999
}
</system>


<artery>
goslow{
fmt:goslow
}
dbghex{
fmt:dbghex
}
</artery>


<supply>
usbcam{
fmt:cam
format:bggr
width:640
height:480
}
</supply>


<relation>
(usbcam, 0, dst) -> (goslow, 0, src)
(goslow, 0, dst) -> (client, 0, src)

(usbcam, 0, dst) -> (dbghex, 0, src)
</relation>
